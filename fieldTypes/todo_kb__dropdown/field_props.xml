							<label>LLL:EXT:kb_kickstarter/fieldTypes/string/locallang.xml:field.prop_eval</label>
							<config>
								<type>select</type>
								<size>8</size>
								<maxitems>20</size>
								<minitems>0</size>
								<items>
									<!-- TODO: Add LLL labels for various eval-types -->
