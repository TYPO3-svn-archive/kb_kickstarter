			<ROOT>
				<TCEforms>
					<sheetTitle>LLL:EXT:kb_kickstarter/llxml/locallang_db_fields.xml:xml.sheet.basic</sheetTitle>
				</TCEforms>
				<type>array</type>
				<el>
					<prop_size type="array">
						<TCEforms>
							<label>LLL:EXT:kb_kickstarter/fieldTypes/string/locallang.xml:field.prop_size</label>
							<config>
								<type>input</type>
								<size>10</size>
								<eval>int</eval>
								<range>
									<upper>200</upper>
									<lower>0</lower>
								</range>
								<default>30</default>
							</config>
						</TCEforms>
					</prop_size>
					<prop_max type="array">
						<TCEforms>
							<label>LLL:EXT:kb_kickstarter/fieldTypes/string/locallang.xml:field.prop_max</label>
							<config>
								<type>input</type>
								<size>10</size>
								<eval>int</eval>
								<range>
									<upper>200</upper>
									<lower>0</lower>
								</range>
								<default>200</default>
							</config>
						</TCEforms>
					</prop_max>
					<prop_default type="array">
