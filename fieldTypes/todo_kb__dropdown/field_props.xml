									</item>
									<item>
										<label>alphanum</label>
										<value>alphanum</value>
									</item>
									<item>
										<label>alphanum_x</label>
										<value>alphanum_x</value>
									</item>
									<item>
										<label>nospace</label>
										<value>nospace</value>
									</item>
									<item>
										<label>md5</label>
										<value>md5</value>
									</item>
									<item>
										<label>is_in</label>
										<value>is_in</value>
									</item>
									<item>
										<label>password</label>
