							<label>LLL:EXT:kb_kickstarter/fieldTypes/dropdown/locallang.xml:field.renderMode</label>
							<config>
								<type>select</type>
								<size>1</size>
								<items>
									<numIndex index="1" type="array">
										<numIndex index="0">LLL:EXT:kb_kickstarter/fieldTypes/dropdown/locallang.xml:field.renderMode.default</numIndex>
										<numIndex index="1"></numIndex>
									</numIndex>
									<numIndex index="2" type="array">
										<numIndex index="0">LLL:EXT:kb_kickstarter/fieldTypes/dropdown/locallang.xml:field.renderMode.checkbox</numIndex>
										<numIndex index="1">checkbox</numIndex>
									</numIndex>
									<numIndex index="3" type="array">
										<numIndex index="0">LLL:EXT:kb_kickstarter/fieldTypes/dropdown/locallang.xml:field.renderMode.singlebox</numIndex>
										<numIndex index="1">singlebox</numIndex>
									</numIndex>
								</items>
								<default></default>
							</config>
						</TCEforms>
					</renderMode>
					<iconMode type="array">
						<TCEforms>
							<label>LLL:EXT:kb_kickstarter/fieldTypes/dropdown/locallang.xml:field.iconMode</label>
							<config>
								<type>select</type>
								<size>1</size>
								<items>
									<numIndex index="1" type="array">
										<numIndex index="0">LLL:EXT:kb_kickstarter/fieldTypes/dropdown/locallang.xml:field.iconMode.default</numIndex>
										<numIndex index="1"></numIndex>
									</numIndex>
									<numIndex index="2" type="array">
										<numIndex index="0">LLL:EXT:kb_kickstarter/fieldTypes/dropdown/locallang.xml:field.iconMode.suppress</numIndex>
