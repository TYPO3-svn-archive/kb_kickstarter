<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<T3DataStructure>
	<meta type="array">
		<langDisable type="integer">1</langDisable>
	</meta>
	<sheets>
		<sDEF>
			<ROOT>
				<TCEforms>
					<sheetTitle>LLL:EXT:kb_kickstarter/llxml/locallang_db_fields.xml:xml.sheet.basic</sheetTitle>
				</TCEforms>
				<type>array</type>
				<el>
					<size type="array">
						<TCEforms>
							<label>LLL:EXT:kb_kickstarter/fieldTypes/dropdown/locallang.xml:field.size</label>
							<config>
								<type>input</type>
								<size>10</size>
								<eval>int</eval>
								<default>5</default>
							</config>
						</TCEforms>
					</size>
					<autoSizeMax type="array">
						<TCEforms>
							<label>LLL:EXT:kb_kickstarter/fieldTypes/dropdown/locallang.xml:field.autoSizeMax</label>
							<config>
								<type>input</type>
								<size>10</size>
								<eval>int</eval>
								<checkbox>0</checkbox>
								<default>0</default>
							</config>
						</TCEforms>
					</autoSizeMax>
					<maxitems type="array">
						<TCEforms>
							<label>LLL:EXT:kb_kickstarter/fieldTypes/dropdown/locallang.xml:field.maxitems</label>
							<config>
								<type>input</type>
								<size>10</size>
								<eval>int</eval>
								<default>0</default>
								<checkbox>0</checkbox>
							</config>
						</TCEforms>
					</maxitems>
					<minitems type="array">
						<TCEforms>
							<label>LLL:EXT:kb_kickstarter/fieldTypes/dropdown/locallang.xml:field.minitems</label>
							<config>
								<type>input</type>
								<size>10</size>
								<eval>int</eval>
								<default>0</default>
								<checkbox>0</checkbox>
							</config>
						</TCEforms>
					</minitems>
					<multiple type="array">
						<TCEforms>
							<label>LLL:EXT:kb_kickstarter/fieldTypes/dropdown/locallang.xml:field.multiple</label>
							<config>
								<type>check</type>
							</config>
						</TCEforms>
					</multiple>
					<iconsInOptionTags type="array">
						<TCEforms>
							<label>LLL:EXT:kb_kickstarter/fieldTypes/dropdown/locallang.xml:field.iconsInOptionTags</label>
							<config>
								<type>check</type>
							</config>
						</TCEforms>
					</iconsInOptionTags>
					<selectElements type="array">
						<type>array</type>
						<section>1</section>
						<el type="array">
							<selectElement type="array">
								<type>array</type>
								<label>LLL:EXT:kb_kickstarter/fieldTypes/dropdown/locallang.xml:field.selectElement</label>
								<tx_templavoila type="array">
								<title>Box element</title>
								</tx_templavoila>
								<el type="array">
									<label type="array">
										<TCEforms>
											<label>LLL:EXT:kb_kickstarter/fieldTypes/dropdown/locallang.xml:field.selectElement.label</label>
											<config>
												<type>input</type>
												<size>30</size>
												<max>250</max>
											</config>
										</TCEforms>
									</label>
									<value type="array">
										<TCEforms>
											<label>LLL:EXT:kb_kickstarter/fieldTypes/dropdown/locallang.xml:field.selectElement.value</label>
											<config>
												<type>input</type>
												<size>30</size>
												<max>250</max>
											</config>
										</TCEforms>
									</value>
									<icon type="array">
										<TCEforms>
											<label>LLL:EXT:kb_kickstarter/fieldTypes/dropdown/locallang.xml:field.selectElement.icon</label>
											<config>
												<type>group</type>
												<internal_type>file</internal_type>
												<allowed>gif,png,jpg</allowed>
												<disallowed>*</disallowed>
												<max_size>1000</max_size>
												<uploadfolder>uploads/tx_kbkickstarter</uploadfolder>
												<show_thumbs>1</show_thumbs>
												<size>1</size>
												<minitems>0</minitems>
												<maxitems>1</maxitems>
											</config>
										</TCEforms>
									</icon>
								</el>
							</selectElement>
						</el>
					</selectElements>
				</el>
			</ROOT>
		</sDEF>
