									<numIndex index="3" type="array">
										<numIndex index="0">LLL:EXT:kb_kickstarter/fieldTypes/dropdown/locallang.xml:field.iconMode.selected</numIndex>
										<numIndex index="1">selected</numIndex>
									</numIndex>
									<numIndex index="4" type="array">
										<numIndex index="0">LLL:EXT:kb_kickstarter/fieldTypes/dropdown/locallang.xml:field.iconMode.current</numIndex>
										<numIndex index="1">current</numIndex>
									</numIndex>
								</items>
								<default></default>
